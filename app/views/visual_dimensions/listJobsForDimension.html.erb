
<h1>Comparision of <%= @key %> with versions <%= @value1 %> and <%= @value2 %></h1>

<%= @dimensions.size %> jobs with appropriate dimension values found
<br />
<br />

<div id="container">


<%= javascript_tag do %>
 $(document).ready(function(){

 	lineChart(<%=raw @draw %>, "container", "Time", "Success in %", "Job", "Comparison of jobs by dimension values", "based on their language");


});
<%end%>
</div>
<br />
<br />
<table class="table table-striped table-bordered table-hover" id="dimensionTable">
  <tr>
    <th>Job</th>
    <th>Result</th>
    <th>Build</th>
    <th colspan="2">Dimension Values</th>
  </tr>

<% @dimensions.each do |dimension| %>
  <tr>
    <td class="dimension"><%= dimension.job1_id %></td>
    <td><%= dimension.job1_state %></td>
    <td><%= dimension.job1_build_id %></td>
    <td><%= dimension.dim1_val %></td>
    <td><%= dimension.dim2_val %></td>
  </tr>
<% end %>

<br />
