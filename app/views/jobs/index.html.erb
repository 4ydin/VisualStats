<h1>Listing jobs</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Repository</th>
    <th>Commit</th>
    <th>Source</th>
    <th>Source type</th>
    <th>Queue</th>
    <th>Type</th>
    <th>State</th>
    <th>Number</th>
    <th>Config</th>
    <th>Status</th>
    <th>Job</th>
    <th>Worker</th>
    <th>Started at</th>
    <th>Finished at</th>
    <th>Created at</th>
    <th>Updated at</th>
    <th>Tags</th>
    <th>Retries</th>
    <th>Allow failure</th>
    <th>Owner</th>
    <th>Owner type</th>
    <th>Result</th>
    <th>Queued at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @jobs.each do |job| %>
  <tr>
    <td><%= job.id %></td>
    <td><%= job.repository_id %></td>
    <td><%= job.commit_id %></td>
    <td><%= job.source_id %></td>
    <td><%= job.source_type %></td>
    <td><%= job.queue %></td>
    <td><%= job.type %></td>
    <td><%= job.state %></td>
    <td><%= job.number %></td>
    <td><%= job.config %></td>
    <td><%= job.status %></td>
    <td><%= job.job_id %></td>
    <td><%= job.worker %></td>
    <td><%= job.started_at %></td>
    <td><%= job.finished_at %></td>
    <td><%= job.created_at %></td>
    <td><%= job.updated_at %></td>
    <td><%= job.tags %></td>
    <td><%= job.retries %></td>
    <td><%= job.allow_failure %></td>
    <td><%= job.owner_id %></td>
    <td><%= job.owner_type %></td>
    <td><%= job.result %></td>
    <td><%= job.queued_at %></td>
    <td><%= link_to 'Show', job %></td>
    <td><%= link_to 'Edit', edit_job_path(job) %></td>
    <td><%= link_to 'Destroy', job, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

